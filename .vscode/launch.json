{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File with Arguments",
            "type": "debugpy",
            "request": "launch",
            "program": "ble_emulator/test_without_fastapi.py",
            "console": "integratedTerminal",
            "args": "Device1 F0:F0:F0:F0:F0:F0 Device2 F0:F0:F0:F0:F0:F1",
            "justMyCode": false,
        },
        {
            "name": "Briefcase: Dev",
            "type": "debugpy",
            "request": "launch",
            "module": "briefcase",
            "args": [
                "dev",
            ],
            "justMyCode": false,
        },
        {
            "name": "Briefcase: Attach (Connect)",
            "type": "debugpy",
